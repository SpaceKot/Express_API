{
  "name": "api-with-express.js",
  "version": "1.0.0",
  "description": "API для учебного проекта 4-NodeJS",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node dist/main.js",
    "dev": "nodemon --exec \"ts-node --files src/main.ts\"", 
    "dev:inspect": "nodemon -e ts,json --exec node --inspect=localhost:9222 -r ts-node/register src/main.ts",
    "lint": "eslint ./src/**",
    "generate": "prisma generate",
    "lint:fix": "eslint ./src/** --fix",
    "build": "tsc",
    "test": "jest",
    "test:e2e": "jest --config jest.e2e.config.ts --coverage"
  },
  "author": "Maksim Voronov",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "inversify": "^7.9.1",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.2.2",
    "tslog": "^3.3.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mocha": "^10.0.10",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "autocannon": "^8.0.0",
    "clinic": "^13.0.0",
    "jest": "^30.1.3",
    "nodemon": "^3.1.10",
    "prisma": "^6.16.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
